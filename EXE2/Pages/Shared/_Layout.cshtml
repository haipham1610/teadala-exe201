							</ul>
						</div>

						<!-- Header Button Box Start -->
						<div class="header-button-box">
							<!-- Header Btn Start -->
							<!-- Header Btn Start -->
							@if (User.Identity != null && User.Identity.IsAuthenticated)
							{
								<form method="post" asp-page="/Login" asp-page-handler="Logout" style="display:inline;">
									<button type="submit" class="btn-default btn-highlighted">Sign Out</button>
								</form>
							}
							else
							{
								<a class="btn-default btn-highlighted" asp-page="/Login">Login</a>
							}

							<!-- Header Btn End -->
							<!-- Header Btn End -->
						</div>
						<!-- Header Button Box End -->
					</div>
					<!-- Main Menu End -->
					<div class="navbar-toggle"></div>
				</div>
			</nav>
			<div class="responsive-menu"></div>
